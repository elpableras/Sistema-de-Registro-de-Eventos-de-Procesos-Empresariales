<?xml version="1.0" encoding="UTF-8"?>
<service>
	<description> Web service que lee un registro o una despedida
	</description>
	<messageReceivers>
		<messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
			class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
	</messageReceivers>
	<parameter name="ServiceClass">servidor.Echo
	</parameter>
	<operation name="leerRegistro" mep="http://www.w3.org/2004/08/wsdl/in-out" />
	<operation name="despedir" mep="http://www.w3.org/2004/08/wsdl/in-out" />
</service>